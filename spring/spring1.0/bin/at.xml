<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="address" class="com.beans1.Address"  p:city="bejing" p:street="hutong"></bean>
  <bean id="car"  class="com.beans1.Car" p:name="audi" p:age="23"></bean>
  <bean id="person" class="com.beans1.Person" p:name="Tom" autowire="byName"></bean>  <!-- autowire自动装配  byName根据属性名-->
  
  </beans>
 